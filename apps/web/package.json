{
    "name": "@repo/web",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "prepare": "panda codegen",
        "dev": "panda codegen && next dev",
        "build": "panda codegen && next build",
        "build:open-next": "panda codegen && open-next build",
        "start": "next start",
        "lint": "panda codegen && next lint",
        "test": "panda codegen && vitest run",
        "test:watch": "panda codegen && vitest",
        "test:coverage": "panda codegen && vitest run --coverage",
        "typecheck": "panda codegen && tsc --noEmit",
        "clean": "rm -rf .next .open-next"
    },
    "dependencies": {
        "@hookform/resolvers": "^5.2.2",
        "@pandacss/preset-panda": "^1.7.2",
        "@repo/api": "workspace:*",
        "@repo/core": "workspace:*",
        "@repo/db": "workspace:*",
        "better-auth": "^1.4.9",
        "next": "^16.1.1",
        "react": "^19.0.0",
        "react-dom": "^19.0.0",
        "react-hook-form": "^7.69.0",
        "zod": "^4.2.1"
    },
    "devDependencies": {
        "@pandacss/dev": "^1.7.2",
        "@repo/testing": "workspace:*",
        "@repo/tsconfig": "workspace:*",
        "@testing-library/dom": "^10.4.0",
        "@testing-library/jest-dom": "^6.6.3",
        "@testing-library/react": "^16.3.0",
        "@testing-library/user-event": "^14.6.1",
        "@types/node": "^22.10.2",
        "@types/react": "^19.0.2",
        "@types/react-dom": "^19.0.2",
        "@vitejs/plugin-react": "^4.5.2",
        "@vitest/coverage-v8": "^4.0.16",
        "jsdom": "^26.1.0",
        "open-next": "^3.1.3",
        "typescript": "^5.7.2",
        "vitest": "^4.0.16"
    }
